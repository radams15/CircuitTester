DIRECTORY_GRAPH = YES

RECURSIVE              = YES
HIDE_UNDOC_RELATIONS   = NO
HAVE_DOT               = YES
UML_LOOK               = YES

BUILTIN_STL_SUPPORT    = YES
EXTRACT_ALL            = YES

EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES

SOURCE_BROWSER = YES

GENERATE_LATEX = NO

HTML_EXTRA_STYLESHEET = customdoxygen.css
LAYOUT_FILE = DoxygenLayout.xml
