PROJECT_NAME = Circuit Simulator

OUTPUT_DIRECTORY = .
INPUT = ../src/main ../src/main/Analysis ../src/main/Saves ../src/main/UI/Components ../src/main/UI
EXCLUDE = ../src/main/Saves/json.h ../src/main/Saves/json.cpp

GENERATE_TAGFILE = YES
TAGFILES = tagfiles/qtcore.tags=https://doc.qt.io/qt-5/ tagfiles/qtgui.tags=https://doc.qt.io/qt-5/ tagfiles/qtwidgets.tags=https://doc.qt.io/qt-5/ tagfiles/qtxml.tags=https://doc.qt.io/qt-5/ tagfiles/cppreference-doxygen-web.tag.xml=http://en.cppreference.com/w/

GENERATE_HTML = YES
HTML_OUTPUT = docs/
HTML_FILE_EXTENSION = .html

USE_MATHJAX = YES

CALL_GRAPH = YES
