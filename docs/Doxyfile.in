GENERATE_TAGFILE = YES
TAGFILES = tagfiles/qtcore.tags=https://doc.qt.io/qt-5/ \
    tagfiles/qtgui.tags=https://doc.qt.io/qt-5/qtgui/ \
    tagfiles/qtxml.tags=https://doc.qt.io/qt-5/qtxml/

